{
  "name": "zerxa/majestic",
  "description": "Majestic a PHP framework for personal web-site, REST API service, CLI application and other project",
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "Modules\\": "Modules/"
    },
    "files": [
      "Core/Function/devdump.php"
    ]
  },
  "authors": [
    {
      "name": "ZerxaFun aKa Zerxa",
      "email": "ZerxaFun@gmail.com"
    }
  ],
  "require": {
    "illuminate/database": "^12.8.1",
    "ext-gd": "*",
    "ext-curl": "*",
    "firebase/php-jwt": "^6.9",
    "ramsey/uuid": "^4.7",
    "ext-zip": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
